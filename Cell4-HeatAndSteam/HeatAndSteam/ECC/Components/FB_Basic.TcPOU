      </Get>
      <Set Name="Set" Id="{a26f9cc5-fb27-4a91-ba58-c5408f786915}">
        <Declaration><![CDATA[PUBLIC 
VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[
IF (P_bManualMode AND NOT bStaManual) THEN
	bCmdAuto := FALSE;
	bCmdManual := TRUE;

ELSIF (NOT P_bManualMode AND bStaManual) THEN
	bCmdAuto := TRUE;
	bCmdManual := FALSE;
	
END_IF
]]></ST>
        </Implementation>
