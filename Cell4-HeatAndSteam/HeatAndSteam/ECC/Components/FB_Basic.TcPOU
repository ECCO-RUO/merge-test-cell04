    <Property Name="P_bManualMode" Id="{74650e9f-1512-4bde-adb7-996fc7f750f9}">
      <Declaration><![CDATA[PROPERTY PUBLIC P_bManualMode : BOOL]]></Declaration>
      <Get Name="Get" Id="{4b92ff7a-d793-416c-a296-4466e28db595}">
